file = {:Users/robin/Library/Application Support/Mendeley Desktop/Downloaded/Maillet et al. - 2012 - Compareads comparing huge metagenomic experiments.pdf:pdf},
issn = {1471-2105},
journal = {BMC Bioinformatics},
keywords = {Algorithms,Information Storage and Retrieval,Information Storage and Retrieval: methods,Metagenomics,Metagenomics: methods},
pages = {S10},
pmid = {23282463},
title = {{Compareads: comparing huge metagenomic experiments.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3526429\&tool=pmcentrez\&rendertype=abstract},
volume = {13 Suppl 1},
year = {2012}
}
