issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: metabolism,Arabidopsis: drug effects,Arabidopsis: enzymology,Arabidopsis: immunology,Flagellin,Flagellin: pharmacology,Immunity, Innate,Molecular Sequence Data,Peptides,Peptides: pharmacology,Protein Binding,Protein Binding: drug effects,Protein Kinases,Protein Kinases: metabolism,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: metabolism,Receptors, Pattern Recognition,Receptors, Pattern Recognition: metabolism,Tobacco,Tobacco: drug effects,Tobacco: enzymology,Tobacco: immunology,Tobacco: microbiology},
