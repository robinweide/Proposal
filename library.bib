pages = {1220--34},
pmid = {15105442},
title = {{Comparative analysis of the receptor-like kinase family in Arabidopsis and rice.}},
url = {http://www.plantcell.org/content/16/5/1220.abstract},
volume = {16},
year = {2004}
}
