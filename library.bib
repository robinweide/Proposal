journal = {Nucleic Acids Res.},
keywords = {Cluster Analysis,Databases, Genetic,Evolution, Molecular,Genes,Internet,Models, Genetic,Molecular Sequence Annotation,Multigene Family,Phylogeny,Proteins,Proteins: classification,Proteins: genetics,Software},
