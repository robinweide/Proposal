journal = {Genome Res.},
keywords = {Algorithms,Animals,Computational Biology,Computational Biology: methods,Genome,Genome, Bacterial,Genome, Bacterial: genetics,Genomics,Genomics: methods,Humans,Internet,Reproducibility of Results,Sequence Analysis, DNA},
