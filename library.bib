keywords = {Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: genetics,Arabidopsis Proteins: metabolism,Arabidopsis: growth \& development,Arabidopsis: metabolism,Arabidopsis: physiology,Cell Death,Cell Death: physiology,Phytosterols,Phytosterols: metabolism,Plant Growth Regulators,Plant Growth Regulators: metabolism,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: genetics,Protein-Serine-Threonine Kinases: metabolism,Signal Transduction,Signal Transduction: physiology},
month = jul,
number = {13},
pages = {1109--15},
pmid = {17600708},
title = {{BAK1 and BKK1 regulate brassinosteroid-dependent growth and brassinosteroid-independent cell-death pathways.}},
url = {http://dx.doi.org/10.1016/j.cub.2007.05.036},
volume = {17},
