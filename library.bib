journal = {Annu. Rev. Phytopathol.},
keywords = {hormone,pathogenesis,plant,plant immunity,plant pathogen,stomata,type iii effector},
month = may,
number = {May},
pmid = {23725467},
title = {{Pseudomonas syringae pv. tomato DC3000: A Model Pathogen for Probing Disease Susceptibility and Hormone Signaling in Plants.}},
