volume = {20},
year = {2008}
}
@article{Jelenska2010,
abstract = {Plant heat shock protein Hsp70 is the major target of HopI1, a virulence effector of pathogenic Pseudomonas syringae. Hsp70 is essential for the virulence function of HopI1. HopI1 directly binds Hsp70 through its C-terminal J domain and stimulates Hsp70 ATP hydrolysis activity in vitro. In plants, HopI1 forms large complexes in association with Hsp70 and induces and recruits cytosolic Hsp70 to chloroplasts, the site of HopI1 localization. Deletion of a central P/Q-rich repeat region disrupts HopI1 virulence but not Hsp70 interactions or association with chloroplasts. Thus, HopI1 must not only bind Hsp70 through its J domain, but likely actively affects Hsp70 activity and/or specificity. At high temperature, HopI1 is dispensable for P. syringae pathogenicity, unless excess Hsp70 is provided. A working hypothesis is that Hsp70 has a defense-promoting activity(s) that HopI1 or high temperature can subvert. Enhanced susceptibility of Hsp70-depleted plants to nonpathogenic strains of P. syringae supports a defense-promoting role for Hsp70.},
