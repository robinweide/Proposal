issn = {1040-4651},
journal = {Plant Cell},
keywords = {Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: genetics,Arabidopsis: enzymology,Gene Duplication,Multigene Family,Oryza sativa,Oryza sativa: enzymology,Oryza sativa: genetics,Phylogeny,Plant Proteins,Plant Proteins: genetics,Protein Kinases,Protein Kinases: genetics},
month = may,
