keywords = {Abscisic Acid,Abscisic Acid: metabolism,Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: genetics,Arabidopsis Proteins: physiology,Arabidopsis: genetics,Arabidopsis: metabolism,Gene Expression Regulation, Plant,Gene Expression Regulation, Plant: physiology,Mannitol,Mannitol: metabolism,Osmotic Pressure,RNA-Binding Proteins,RNA-Binding Proteins: genetics,RNA-Binding Proteins: physiology,Sodium Chloride,Sodium Chloride: metabolism},
month = jan,
