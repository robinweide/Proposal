pmid = {20601439},
title = {{Next-generation sequencing techniques for eukaryotic microorganisms: sequencing-based solutions to biological problems.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2937339\&tool=pmcentrez\&rendertype=abstract},
volume = {9},
year = {2010}
}
