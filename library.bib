journal = {Plant Cell},
keywords = {Amino Acid Sequence,Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: genetics,Arabidopsis: genetics,Arabidopsis: microbiology,Base Sequence,Conserved Sequence,Conserved Sequence: genetics,DNA, Complementary,DNA, Complementary: genetics,Gene Expression Regulation, Plant,Gene Expression Regulation, Plant: genetics,Genes, Plant,Genes, Plant: genetics,Genome, Plant,Immunity, Innate,Immunity, Innate: genetics,Molecular Sequence Data,Plant Diseases,Plant Diseases: genetics,Plant Proteins,Plant Proteins: genetics,Plant Proteins: isolation \& purification,Plant Proteins: metabolism,Proto-Oncogene Proteins c-myb,Proto-Oncogene Proteins c-myb: genetics,Proto-Oncogene Proteins c-myb: isolation \& purific,Proto-Oncogene Proteins c-myb: metabolism,Species Specificity},
