issn = {1553-7374},
journal = {PLoS Pathog.},
keywords = {Amino Acid Motifs,Arabidopsis,Arabidopsis Proteins,Arabidopsis Proteins: metabolism,Arabidopsis: cytology,Arabidopsis: genetics,Arabidopsis: immunology,Arabidopsis: metabolism,Cell Membrane,Cell Membrane: metabolism,Cell Wall,Cell Wall: metabolism,Fungal Proteins,Fungal Proteins: chemistry,Fungal Proteins: metabolism,Gene Expression,Immunity, Innate,Mutagenesis, Site-Directed,Mutation,Phenotype,Phytophthora infestans,Phytophthora infestans: genetics,Phytophthora infestans: metabolism,Phytophthora infestans: pathogenicity,Plant Diseases,Plant Diseases: immunology,Plants, Genetically Modified,Polymerase Chain Reaction,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: metabolism},
month = mar,
