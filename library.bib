file = {:Users/robin/Library/Application Support/Mendeley Desktop/Downloaded/Zeng et al. - 2012 - A tomato LysM receptor-like kinase promotes immunity and its kinase activity is inhibited by AvrPtoB.pdf:pdf},
issn = {1365-313X},
journal = {Plant J.},
keywords = {Amino Acid Motifs,Amino Acid Substitution,Arabidopsis Proteins,Arabidopsis Proteins: chemistry,Bacterial Proteins,Bacterial Proteins: metabolism,Host-Pathogen Interactions,Lycopersicon esculentum,Lycopersicon esculentum: genetics,Lycopersicon esculentum: immunology,Lycopersicon esculentum: metabolism,Lycopersicon esculentum: microbiology,Molecular Mimicry,Molecular Sequence Data,Plant Proteins,Plant Proteins: chemistry,Plant Proteins: immunology,Plant Proteins: metabolism,Plants, Genetically Modified,Protein-Serine-Threonine Kinases,Protein-Serine-Threonine Kinases: chemistry,Protein-Serine-Threonine Kinases: immunology,Protein-Serine-Threonine Kinases: metabolism,Pseudomonas syringae,Pseudomonas syringae: pathogenicity,Two-Hybrid System Techniques},
